{
  "name": "react2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "http-server": "^0.10.0",
    "node-sass-chokidar": "0.0.3",
    "react": "^15.6.1",
    "react-burger-menu": "^2.1.5",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-scripts": "1.0.12",
    "react-theme-provider": "^0.1.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start webpack-dev-server --progress --colors --hot --config ./webpack.config.js",
    "build": "react-scripts build ./webpack.js -p",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "devDependencies": {
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "react-hot-loader": "^3.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "styled-components": "^2.1.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
